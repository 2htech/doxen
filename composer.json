{
  "name": "tlapnet/doxen",
  "license": "proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.ispalliance.cz/"
    }
  ],
  "require": {
    "erusev/parsedown": "~1.6.4",
    "nette/application": "~2.4.9"
  },
  "require-dev": {
    "ninjify/qa": "~0.4.0",
    "ninjify/nunjuck": "~0.2.0"
  },
  "autoload": {
    "psr-4": {
      "Tlapnet\\Doxen\\": "src/"
    }
  },
  "scripts": {
    "qa": [
      "linter src",
      "codesniffer src"
    ],
    "tester": [
      "tester -s -p php --colors 1 -C tests/cases"
    ],
    "phpstan": [
      "phpstan analyze --level 5 src tests"
    ]
  }
}
